{
	"name": "universal-block",
	"version": "0.1.1",
	"private": true,
	"description": "A designless polymorphic WordPress block that can be any HTML element",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build src/index.js --output-path=build",
		"start": "wp-scripts start src/index.js --output-path=build",
		"dev": "npm run start",
		"lint:js": "wp-scripts lint-js src/",
		"lint:css": "wp-scripts lint-style assets/css/",
		"format:js": "wp-scripts format-js src/",
		"test": "wp-scripts test-unit-js"
	},
	"keywords": [
		"wordpress",
		"gutenberg",
		"block",
		"polymorphic",
		"designless"
	],
	"author": "Your Name",
	"license": "GPL-2.0-or-later",
	"devDependencies": {
		"@wordpress/scripts": "^28.5.0"
	},
	"dependencies": {
		"@wordpress/blocks": "^12.0.0",
		"@wordpress/block-editor": "^12.0.0",
		"@wordpress/components": "^25.0.0",
		"@wordpress/element": "^5.0.0",
		"@wordpress/i18n": "^4.0.0"
	}
}